{
  "extends": ["eslint:recommended", "plugin:jest/recommended", "next/core-web-vitals"],
  "plugins": ["jest"],
  "rules": {
    "react/jsx-indent": [
      "error",
      2,
      {
        "checkAttributes": true,
        "indentLogicalExpressions": true
      }
    ],
    "react/jsx-indent-props": ["error", 2],
    "indent": ["error", 2]
  }
}
